{
  "source": {
    "include": ["./src"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(node_modules|\\.git)"
  },
  "opts": {
    "destination": "./docs",
    "template": "node_modules/docdash",
    "css": "./css/jsdoc.css"
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}
